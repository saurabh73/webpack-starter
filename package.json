{
  "name": "webpack-starter",
  "description": "Webpack Starter for basic frontend stack",
  "license": "ISC",
  "author": "Saurabh Dutta (saurabh73)",
  "homepage": "https://github.com/saurabh73/webpack-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saurabh73/webpack-starter.git"
  },
  "bugs": {
    "url": "https://github.com/saurabh73/webpack-starter/issues"
  },
  "version": "1.0.0",
  "scripts": {
    "build": "npm run lint && cross-env NODE_ENV=production webpack --mode production --config webpack.config.js",
    "dev": "webpack-dev-server --mode development --open --config webpack.config.js",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "start": "npm run dev"
  },
  "dependencies": {
    "bootstrap": "^4.2.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.6",
    "purgecss-webpack-plugin": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.0.0",
    "css-url-relative-plugin": "^1.0.0",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jquery": "^1.3.2",
    "eslint-plugin-prettier": "^3.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "glob": "^7.1.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.0.0-beta.2",
    "husky": "^1.2.0",
    "image-webpack-loader": "^4.6.0",
    "lint-staged": "^8.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "preload-webpack-plugin": "^3.0.0-beta.3",
    "prettier": "^1.15.3",
    "prettier-package-json": "^2.0.1",
    "resolve-url-loader": "^2.3.1",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "keywords": [
    "webpack",
    "webpack boilerplate",
    "webpack bootstrap jquery",
    "webpack jquery",
    "webpack starter"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "package.json": [
      "prettier-package-json --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
